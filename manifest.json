{
  "name": "ClassTime",
  "version": "1.0",
  "description": "Keep track of links for online classes.",
  "permissions": ["storage"],
  "browser_action": {
      "default_title": "My Bookmarks",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "options_page": "options.html",
  "manifest_version": 2
}
